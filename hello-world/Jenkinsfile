pipeline {
    agent any 
    stages {
        stage('Static Analysis') {
            steps {
                echo 'Run the static analysis to the code' 
            }
        }
        stage('Compile') {
            steps {
                echo 'Compile the source code for secnd time' 
            }
        }
        stage('Security Check') {
            steps {
                echo 'Run the security check against the application' 
            }
        }
        stage('Run Unit Tests') {
            steps {
                echo 'Run unit tests again' 
            }
        }
        stage('Publish Artifacts') {
            steps {
                echo 'Removed previous stage of "run integration test" Save the assemblies generated from the compilation' 
            }
        }
    }
}
